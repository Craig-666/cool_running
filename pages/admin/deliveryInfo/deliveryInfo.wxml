<!--pages/admin/deliveryInfo/deliveryInfo.wxml-->
<view class='emp-info'>姓名:{{name}} 电话:{{phone}}</view>
<view class='list'>
  <view class='item' bindtap='tap' data-objid='{{item.objectId}}' wx:for='{{delveryList}}' wx:key='index'>
    <view>第{{index+1}}趟</view>
    <view>{{item.order_count}}单</view>
    <view>{{item.createdAt}}</view>
  </view>
</view>
