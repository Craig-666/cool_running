/* pages/admin/abnormal/abnormal.wxss */

.total{
  line-height: 80rpx;
  position: fixed;
  top: 0;
  border-bottom: 2rpx solid #efeff4;
  padding-left: 30rpx;
  background-color: white;
  z-index: 2;
  width: 100%;
}

.search-list{
  padding-top: 82rpx
}
.search-list .item{
  padding: 0 30rpx 0 30rpx;
}

.bottom{
	height: 6rpx;
	width: 100%;
	background-color: #efeff4
}

.search-list .item .info{
  display: flex;
  justify-content: space-between;
  line-height: 80rpx;
  border-bottom: 1rpx solid #efeff4
}

.handle{
	width: 80rpx;
	display: flex;
	align-items: center;
	justify-content: center;
	background-color: #00a800;
	color: white
}